---
layout: container
title: Daz Studio Content
order: 30
icon: cubes
permalink: /daz-studio-content/
#group: navigation
---
<ul class="nav nav-pills nav-stacked col-md-3">
    {% for daz in site.daz-studio-content %}
    <li {% if forloop.first %} class="active" {% endif %}><a href="#{{ daz.id }}" data-toggle="pill">{{ daz.title }}</a></li>
    {% endfor %}
</ul>
<div class="tab-content col-md-9">
    {% for daz in site.daz-studio-content %}
    <div class="tab-pane fade {% if forloop.first %} in active {% endif %}>" id="{{ daz.id }}">
        <h2>{{ daz.title }}</h2>
        {{ daz.content }}
        
        {% if daz.details %}
        <div class="clearfix">
            <div class="pull-right">
                <i class="fa fa-download"></i> Download on:
                <div class="btn-toolbar" role="toolbar" aria-label="...">
                  <div class="btn-group btn-group-lg" role="group" aria-label="...">
                      {% if daz.details.renderosity %}
                      <a class="btn btn-default" href="{{daz.details.renderosity}}?AID=4909">Renderosity</a>
                      {% endif %}
                      {% if daz.details.sharecg %}
                      <a class="btn btn-default" href="{{daz.details.sharecg}}">ShareCG</a>
                      {% endif %}
                  </div>
                </div>
            </div>
        </div>
        {% endif %}
    </div>
    {% endfor %}
</div>